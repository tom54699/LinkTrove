{
  "manifest_version": 3,
  "name": "LinkTrove",
  "key": "",
  "version": "0.1.0",
  "description": "Toby-like visual tab manager: drag from Open Tabs to save as cards.",
  "action": {
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["tabs", "storage", "scripting", "identity"],
  "host_permissions": ["<all_urls>", "https://www.googleapis.com/*"],
  "oauth2": {
    "client_id": "731462500420-80ko62k4mmb74jajefi7fio2jftlrc61.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive.appdata"
    ]
  }
}
